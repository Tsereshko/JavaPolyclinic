<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="733.0" prefWidth="656.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
>
    <children>
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="16.0" AnchorPane.bottomAnchor="20.0"
              AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="20.0">
            <children>
                <VBox fx:id="labelVBox" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308"
                      maxWidth="1.7976931348623157E308" spacing="25.0">
                    <children>
                        <Label fx:id="nameLabel" text="Имя: ">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="surnameLabel" text="Фамилия: ">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="patronymicLable" text="Отчество: ">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="genderLabel" text="Пол: ">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="phoneLabel" text="Телефон: ">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <VBox.margin>
                                <Insets top="15.0"/>
                            </VBox.margin>
                        </Label>
                        <Label fx:id="addressLabel" text="Адресс проживания: ">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label fx:id="dateLabel" text="Дата рождения: ">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                    </children>
                </VBox>
                <VBox fx:id="fillVBox" alignment="TOP_CENTER" maxHeight="1.7976931348623157E308"
                      maxWidth="1.7976931348623157E308">
                    <children>
                        <TextField fx:id="nameTextField"/>
                        <Label fx:id="nameError" textFill="#ff7171">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="surnameTextField"/>
                        <Label fx:id="surnameError" textFill="#ff7171">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="patronymicTextField"/>
                        <Label fx:id="patronymicError" textFill="#ff7171">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <RadioButton fx:id="manRadioButton" mnemonicParsing="false" text="Мужской"/>
                        <RadioButton fx:id="womanRadioButton" mnemonicParsing="false" text="Женский">
                            <VBox.margin>
                                <Insets bottom="16.0" top="16.0"/>
                            </VBox.margin>
                        </RadioButton>
                        <TextField fx:id="phoneTextField"/>
                        <Label fx:id="phoneError" textFill="#ff7171">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <TextField fx:id="addressTextField"/>
                        <Label fx:id="addressError" textFill="#ff7171">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <DatePicker fx:id="datePicker" editable="false"/>
                    </children>
                </VBox>
            </children>
        </HBox>
        <JFXButton text="Кнопка" fx:id="buttonButton" layoutX="270.0" layoutY="707.0" onAction="#buttonAction"
                   prefHeight="25.0" prefWidth="176.0" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="200.0"
                   AnchorPane.rightAnchor="200.0">
            <font>
                <Font size="16.0"/>
            </font>
        </JFXButton>
        <Label fx:id="dateError" alignment="CENTER" contentDisplay="CENTER" layoutX="327.0" layoutY="707.0"
               textFill="#ff7171" AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0">
            <font>
                <Font size="16.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
