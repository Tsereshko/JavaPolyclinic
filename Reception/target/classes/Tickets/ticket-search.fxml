                        </VBox>
                    </children>
                </HBox>
                <JFXButton fx:id="searchButton" onAction="#searchAction" text="ПОИСК">
                    <font>
                        <Font size="16.0" />
                    </font>
                </JFXButton>
            </children>
        </VBox>
    </children>
</AnchorPane>
